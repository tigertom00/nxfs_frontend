# Production Environment Variables
# Copy this file to .env.production and fill in your actual values

# API Configuration
NEXT_PUBLIC_API_URL=https://api.nxfs.no
NEXT_PUBLIC_N8N_URL=https://n8n.nxfs.no/webhook/nxfs

# N8N Chatbot Integration
# IMPORTANT: Replace with your actual production secret key
NEXT_PUBLIC_N8N_SECRET_KEY=your-production-secret-key-here

# API Authentication
NEXT_PUBLIC_API_TOKEN=your_production_api_token_here

# Playwright Testing Credentials (Production Testing)
PLAYWRIGHT_TEST_EMAIL=your_production_test_email
PLAYWRIGHT_TEST_PASSWORD=your_production_test_password

# Node Environment
NODE_ENV=production